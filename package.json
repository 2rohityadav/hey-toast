{
  "name": "hey-toast",
  "version": "1.0.0",
  "description": "hey-toast notification - Raise a toast to better notifications - web component built with Stenciljs",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hey-toast/hey-toast.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2rohityadav/hey-toast.git"
  },
  "files": [
    "dist/",
    "loader/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "generate": "stencil generate",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "commit": "git add . && git commit",
    "commit:all": "git add . && git commit -m"
  },
  "dependencies": {
    "@stencil/core": "4.36.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^29.0.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "semantic-release": "^24.2.8",
    "ts-jest": "^29.4.1"
  },
  "license": "MIT"
}
